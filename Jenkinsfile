            steps {
                echo 'running unit test'
            }
        }
        stage('automation test') {
            steps {
                echo 'running automation test'
            }
        }
        stage('checkout code') {
            steps {
                echo 'checking out code'
            }
        }
        stage('code quality') {
            steps {
                echo 'checking code quality'
            }
        }
    }
}
